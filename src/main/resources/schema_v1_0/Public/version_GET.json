{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "title": "version_GET",
    "description": "Schema for Version",
    "type": "object",
    "properties": {
        "version_id": {
            "type": "string",
            "required": true
        },
        "detailed_version": {
            "type": "string",
            "required": false
        }
    },
    "links": [
        {
            "rel": "projects",
            "href": "{version_id}/projects"
        }
    ]
}
